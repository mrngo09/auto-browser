{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch TypeScript Cron Job",
        "program": "${workspaceFolder}/src/play-iwin-bio/index.ts",
        "preLaunchTask": "yarn playiwinbio",
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "sourceMaps": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }